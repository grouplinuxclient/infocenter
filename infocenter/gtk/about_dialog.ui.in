<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <object class="AdwAboutDialog" id="about_dialog">
    <property name="version">@VERSION@</property>
    <property name="copyright">@COPYRIGHT@</property>
    <property name="website">@PACKAGE_URL@</property>
    <property name="developers">@CONTRIBUTORS@</property>
    <property name="developer_name">Jan-Michael Brummer</property>
    <property name="translator_credits" translatable="yes" comments="Add your name to the translator credits list">translator-credits</property>
    <property name="designers">@ARTISTS@</property>
    <property name="application_name" translatable="yes">Information Center</property>
    <property name="application_icon">@APPID@</property>
    <property name="license_type">gpl-2-0</property>
    <property name="issue_url">https://github.com/grouplinuxclient/infocenter/issues</property>
  </object>
</interface>
